<main class="apps-ocio-page">

<h1>Apps de Ocio</h1>

    @foreach (AppOcio item in @ViewBag.listaAppsNoAsociadas)
    {
        <div>
            <img src="/Imagenes/@item.foto/" />

            @item.titulo

            <a href='@Url.Action("eliminarAppOcio", "Home", new { IDAppOcio = item.id })'>


                <img src="/Imagenes/delete-icon.png" />

            </a>
        </div>
    }

</main>
<!-- fin boton para volver -->